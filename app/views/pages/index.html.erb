<h1>My List</h1>
<table>
  <div class="col-md-6">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Image</th>
          <th>Name</th>
          <th>Author</th>
          <th>Discription</th>

        </tr>
      </thead>
      <tbody>
       <% @follows.each do |follow| %>
       <tr>
         <td><%= image_tag follow.series.image,:class => 'image' %></td>
        <td><%= link_to follow.series.name, follow.series %></td>
        <td><%= follow.series.author %></td>
        <td><%= follow.series.description %></td>

        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</table>
