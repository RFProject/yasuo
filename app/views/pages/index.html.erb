<h1>My List</h1>
<table>
  <div class="col-md-6">
    <table class="table table">
      <thead>
        <tr>
          <th>Name</th>
          <th>Author</th>
          <th>Discription</th>
          <th>Image</th>
        </tr>
      </thead>
      <tbody>
       <% @follows.each do |follow| %>
       <tr>
        <td><%= follow.series.name %></td>
        <td><%= follow.series.author %></td>
        <td><%= follow.series.description %></td>
        <td><%= image_tag follow.series.image %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</table>
